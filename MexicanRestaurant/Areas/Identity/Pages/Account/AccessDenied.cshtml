@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<div class="container text-center my-5">
    <h1 class="display-4 text-danger">@ViewData["Title"]</h1>
    <p class="lead text-muted">You do not have permission to access this resource.</p>
    <a class="btn btn-outline-primary mt-3" asp-area="" asp-controller="Home" asp-action="Index">Return Home</a>
</div>
